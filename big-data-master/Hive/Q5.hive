add jar /home/FORMAT_GENRE_HIVE.jar;
CREATE TEMPORARY FUNCTION FORMAT_GENRE as 'FORMAT_GENRE_HIVE';
DROP TABLE IF EXISTS MOVIES;
CREATE TABLE MOVIES (MOVIEID INT, TITLE STRING, Genre STRING)  ROW FORMAT DELIMITED FIELDS TERMINATED BY '\#';
LOAD DATA  local INPATH '/Fall2014/movies_new.dat' OVERWRITE INTO TABLE movies;

SELECT MOVIEID, TITLE, FORMAT_GENRE(Genre) FROM MOVIES;
